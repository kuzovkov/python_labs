.post .infopanel .voting{float:left;position:relative;padding:0px 20px;margin:6px 0;margin-right:26px;}

/* еще не голосовал - не подсвечиваем ни чего. стрелки обычные, не затененные. */
.post .infopanel .voting .plus{width:11px;height:15px;display:block;position:absolute;left:0;top:0px;}
.post .infopanel .voting .minus{width:11px;height:15px;display:block;position:absolute;right:0;top:0px;}
.post .infopanel .voting a.plus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat scroll left bottom transparent;}
.post .infopanel .voting a.minus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat scroll -12px bottom transparent;}
.post .infopanel .voting.voted a.plus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat scroll right top transparent;}
.post .infopanel .voting.voted a.minus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat scroll right bottom transparent;}
.post .infopanel .voting span.plus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat scroll right top transparent;}
.post .infopanel .voting span.minus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat scroll right bottom transparent;}


.post .infopanel .voting a.plus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat 0px -16px;}
.post .infopanel .voting a.minus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat -12px -16px;}
.post .infopanel .voting a:hover.plus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat 0px 0px ;}
.post .infopanel .voting a:hover.minus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat -12px 0px ;}

/* проголосовал в плюс - подсвечиваем стрелку вверх и затемняем стрелку вниз*/
.post .infopanel .voting.voted_plus a.plus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat scroll left top transparent;}
.post .infopanel .voting.voted_plus span.plus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat scroll left top transparent;}
.post .infopanel .voting.voted_plus a.minus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat scroll right bottom transparent;}
.post .infopanel .voting.voted_plus span.minus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat scroll right bottom transparent;}

/* проголосовал в минус - подсвечиваем стрелку вниз и затемняем стрелку вверх */
.post .infopanel .voting.voted_minus a.plus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat scroll top right transparent;}
.post .infopanel .voting.voted_minus span.plus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat scroll top right transparent;}
.post .infopanel .voting.voted_minus a.minus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat scroll -12px top transparent;}
.post .infopanel .voting.voted_minus span.minus{background:url("/images/icos/icons_vote_posts.gif?r=2") no-repeat scroll -12px top transparent;}

/* общий балл не известен */
.post .infopanel .voting .mark{float:left;color:#A9A9A9;font-weight:bold;font-size:12px;margin-top:1px;}
.post .infopanel .voting .mark span{color:#A9A9A9;}
.post .infopanel .voting .mark a{text-decoration:none;color:#A9A9A9;}
/* общий бал выше нуля */
